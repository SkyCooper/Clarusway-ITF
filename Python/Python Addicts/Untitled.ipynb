    "a = input(\"bir sayı giriniz:\")  #75 False ,171 True\n",
    "sum = 0\n",
    "digit = len(a)\n",
    "for i in range(digit):\n",
    "    sum = sum + int(a[i])\n",
    "if (int(a))%sum == 0:\n",
    "    print(\"True\")\n",
    "else:\n",
    "    print(\"False\")"
